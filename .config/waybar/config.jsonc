{
  "layer": "top",
  "include": [
    "~/.config/waybar/modules.jsonc"
  ],
  "modules-left": [],
  "modules-center": [
    "clock",
    "tray"
  ],
  "modules-right": [
    "group/hardware",
    "network",
    "pulseaudio",
    "battery",
    "custom/power"
  ],
  "group/hardware": {
    "orientation": "horizontal",
    "modules": [
      "cpu",
      "custom/gpu-nvidia",
      "memory"
    ]
  },
  "spacing": 15
}
